{
  "name": "website",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "del-cli \"dist/**/*\" && npm run images & npm run icons & npm run lint",
    "build": "cross-env NODE_env=production webpack --env.environment production --hide-modules",
    "postbuild": "node ./build/critical",
    "deploy": "cross-env NODE_ENV=production npm run push:remote",
    "icons": "node ./build/icons",
    "images": "node ./build/images",
    "lint": "eslint \"src/**/*.js\"",
    "prettier": "prettier --write --single-quote \"src/**/*.js\"",
    "prettier:all": "prettier --write --single-quote \"webpack.config.*.js\" \"build/**/*.js\" \"__tests__/**/*/js\" \"__mocks__/**/*/js\"",
    "prepush:remote": "npm run build",
    "push:remote": "node ./build/deploy",
    "start": "webpack-dev-server --env.environment development --hot --quiet --inline --content-base . --content-base static",
    "test": "jest --no-watchman --no-cache",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "autosize": "~3.0.20",
    "es6-promise": "~4.1.0",
    "intersection-observer": "~0.2.1",
    "ityped": "~0.0.10",
    "minireset.css": "~0.0.3",
    "normalize.css": "~7.0.0",
    "offline-plugin": "~4.8.1",
    "particles.js": "dschau/particles.js#f421a5bd31efd572842645884c803c0c09d364fe",
    "smoothscroll-polyfill": "~0.3.5",
    "unfetch": "~2.1.2",
    "webfontloader": "~1.6.28"
  },
  "devDependencies": {
    "babel-eslint": "~7.2.3",
    "cheerio": "~1.0.0-rc.1",
    "config": "~1.26.1",
    "cross-env": "~5.0.1",
    "del": "~2.2.2",
    "del-cli": "~1.0.0",
    "eslint": "~3.19.0",
    "eslint-config-airbnb-base": "~11.2.0",
    "eslint-import-resolver-webpack": "~0.8.1",
    "eslint-plugin-import": "~2.3.0",
    "glob": "~7.1.1",
    "gm": "~1.23.0",
    "husky": "~0.13.3",
    "jest": "~20.0.3",
    "lint-staged": "~3.4.2",
    "mkdirp": "~0.5.1",
    "mz": "~2.6.0",
    "prettier": "~1.3.1",
    "promise-ftp": "~1.3.3",
    "recursive-readdir": "~2.2.1",
    "sharp": "^0.18.0",
    "webpack": "~2.6.1",
    "webpack-config": "dschau/webpack-config",
    "webpack-dev-server": "~2.4.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  }
}
