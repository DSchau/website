{
  "name": "website",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "del-cli \"dist/**/*\" && npm run copy",
    "build": "cross-env NODE_env=production webpack --env.environment production",
    "predeploy": "npm run build",
    "deploy": "cross-env NODE_ENV=production node ./scripts/deploy",
    "copy": "cpy \"static/*.json\" \"dist\"",
    "prettier": "prettier --write --single-quote \"src/**/*.js\"",
    "prettier:all": "prettier --write --single-quote \"webpack.config.*.js\" \"scripts/**/*.js\" \"__tests__/**/*/js\" \"__mocks__/**/*/js\"",
    "start": "webpack-dev-server --env.environment development --hot --quiet --content-base . --content-base static",
    "test": "jest --no-watchman",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "autosize": "~3.0.20",
    "es6-promise": "~4.1.0",
    "ityped": "~0.0.10",
    "normalize.css": "~7.0.0",
    "particles.js": "dschau/particles.js#6ad91e1",
    "smoothscroll-polyfill": "~0.3.5",
    "supports-webp": "~1.0.5",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "~7.1.0",
    "babel-core": "~6.24.1",
    "babel-loader": "~7.0.0",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "babel-preset-env": "~1.4.0",
    "config": "~1.26.1",
    "cpy-cli": "~1.0.1",
    "cross-env": "~5.0.0",
    "css-loader": "~0.28.1",
    "del-cli": "~1.0.0",
    "extract-text-webpack-plugin": "~2.1.0",
    "file-loader": "~0.11.1",
    "glob": "~7.1.1",
    "gm": "~1.23.0",
    "html-webpack-plugin": "~2.28.0",
    "husky": "~0.13.3",
    "jest": "~20.0.3",
    "lint-staged": "~3.4.2",
    "node-sass": "~4.5.2",
    "postcss": "~6.0.1",
    "postcss-loader": "~2.0.5",
    "prettier": "~1.3.1",
    "promise-ftp": "~1.3.3",
    "pug": "~2.0.0-rc.1",
    "pug-loader": "~2.3.0",
    "recursive-readdir": "~2.2.1",
    "sass-loader": "~6.0.5",
    "script-loader": "~0.7.0",
    "style-loader": "~0.17.0",
    "url-loader": "~0.5.8",
    "webpack": "~2.5.1",
    "webpack-config-assign": "~1.0.0",
    "webpack-dev-server": "~2.4.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote",
      "git add"
    ]
  }
}
